{% extends 'adminbase.html.twig' %}

{% block title %}Oeuvre{% endblock %}

{% block body %}
    <h1>Oeuvre</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ oeuvre.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ oeuvre.image }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ oeuvre.description }}</td>
            </tr>
            <tr>
                <th>NomArtiste</th>
                <td>{{ oeuvre.nomArtiste }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ oeuvre.dateCreation ? oeuvre.dateCreation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ oeuvre.reference }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ oeuvre.prix }}</td>
            </tr>
            <tr>
                <th>NomOeuvre</th>
                <td>{{ oeuvre.nomOeuvre }}</td>
            </tr>
            <tr>
                <th>TypeOeuvre</th>
                <td>{{ oeuvre.typeOeuvre }}</td>
            </tr>
            <tr>
                <th>Linkhttp</th>
                <td>{{ oeuvre.linkhttp }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_oeuvre_index') }}">back to list</a>

    <a href="{{ path('app_oeuvre_edit', {'id': oeuvre.id}) }}">edit</a>

    {{ include('oeuvre/_delete_form.html.twig') }}
{% endblock %}
