{% extends 'base_front.html.twig' %}

{% block body %}
    <div class="container">
        <h1>Créer un nouvel élément</h1>
        <div class="row">
            <div class="col-md-6">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                <button type="submit" class="btn btn-primary">Créer</button>
                <a href="{{ path('app_adresse_index') }}" class="btn btn-secondary">Annuler</a>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}

{% block title %}
    Créer un nouvel élément

{% endblock %}
